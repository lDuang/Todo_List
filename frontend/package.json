{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "preview": "vite preview",
    "build": "tsc -b && vite build",
    "build:all": "pnpm build:vite && pnpm build:electron",
    "build:vite": "tsc -b && vite build",
    "build:electron": "tsc -p electron/tsconfig.electron.json",
    "start:dev": "electron dist/electron/main.js --dev",
    "start:prod": "electron dist/electron/main.js",
    "electron:dev": "concurrently \"pnpm dev\" \"wait-on tcp:3000 && pnpm build:electron && pnpm start:dev\"",
    "electron:build": "pnpm build && electron-builder",
    "electron": "pnpm build && pnpm start:prod"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-slot": "^1.2.3",
